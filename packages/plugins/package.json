  },
  "dependencies": {
    "@auto-cesium/core": "workspace:*",
    "@auto-cesium/shared": "workspace:*",
    "@turf/turf": "^7.2.0",
    "shpjs": "^6.2.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitest/ui": "^1.0.0",
    "cesium": "^1.135.0",
    "happy-dom": "^12.10.3",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.0.0"
  }
