{
  "name": "@ktd-cesium/shared",
  "version": "1.0.0",
  "description": "Cesium共享基础库",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc --emitDeclarationOnly --outDir dist",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "cesium",
    "shared",
    "utils"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "cesium": "^1.135.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.0.0",
    "cesium": "^1.135.0",
    "happy-dom": "^20.0.10",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
