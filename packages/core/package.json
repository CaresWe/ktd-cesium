      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@ktd-cesium/shared": "workspace:*"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.0.0",
    "cesium": "^1.135.0",
    "happy-dom": "^20.0.10",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0"
  }
